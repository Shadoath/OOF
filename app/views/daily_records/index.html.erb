<h1>Listing daily records</h1>

<table>
  <thead>
    <tr>
      <th>Daily date</th>
      <th>Temp low</th>
      <th>Temp high</th>
      <th>Eggs collected</th>
      <th>Total ducks</th>
      <th>Hours of daylight</th>
      <th>Sunrise</th>
      <th>Moon phase</th>
      <th>Moon percent</th>
      <th>Weather</th>
      <th>Rained</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_records.each do |daily_records| %>
      <tr>
        <td><%= daily_records.daily_date %></td>
        <td><%= daily_records.temp_low %></td>
        <td><%= daily_records.temp_high %></td>
        <td><%= daily_records.eggs_collected %></td>
        <td><%= daily_records.total_ducks %></td>
        <td><%= daily_records.hours_of_daylight %></td>
        <td><%= daily_records.sunrise %></td>
        <td><%= daily_records.moon_phase_id %></td>
        <td><%= daily_records.moon_percent %></td>
        <td><%= daily_records.weather %></td>
        <td><%= daily_records.rained %></td>
        <td><%= link_to 'Show', daily_records %></td>
         <td><%= link_to 'Edit', edit_daily_records_path(daily_records) %></td>
        <td><%= link_to 'Destroy', daily_records, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Daily data', new_daily_record_path %>


