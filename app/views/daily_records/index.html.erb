<h1>Listing daily records</h1>

<table>
  <thead>
    <tr>
      <th>Daily date</th>
      <th>Temp low</th>
      <th>Temp high</th>
      <th>Eggs collected</th>
      <th>Total ducks</th>
      <th>Hours of daylight</th>
      <th>Sunrise</th>
      <th>Moon phase</th>
      <th>Moon percent</th>
      <th>Weather</th>
      <th>Precipitation <br>(0-10)</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_records.each do |daily_record| %>
      <tr>
        <td><%= daily_record.daily_date %></td>
        <td><%= daily_record.temp_low %></td>
        <td><%= daily_record.temp_high %></td>
        <td><%= daily_record.eggs_collected %></td>
        <td><%= daily_record.total_ducks %></td>
        <td><%= daily_record.hours_of_daylight %></td>
        <td><%= daily_record.sunrise %></td>
        <td><%= daily_record.moon_phase_id %></td>
        <td><%= daily_record.moon_percent %></td>
        <td><%= daily_record.weather %></td>
        <td><%= daily_record.precipitation %></td>
        <td><%= link_to 'Show', daily_record %></td>
         <td><%= link_to 'Edit', edit_daily_record_path(daily_record) %></td>
        <td><%= link_to 'Destroy', daily_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Daily Record', new_daily_record_path %>


