<h1>Listing daily_data</h1>

<table>
  <thead>
    <tr>
      <th>Daily date</th>
      <th>Temp low</th>
      <th>Temp high</th>
      <th>Eggs collected</th>
      <th>Total ducks</th>
      <th>Hours of daylight</th>
      <th>Sunrise</th>
      <th>Moon phase</th>
      <th>Moon percent</th>
      <th>Weather</th>
      <th>Rained</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_data.each do |daily_datum| %>
      <tr>
        <td><%= daily_datum.daily_date %></td>
        <td><%= daily_datum.temp_low %></td>
        <td><%= daily_datum.temp_high %></td>
        <td><%= daily_datum.eggs_collected %></td>
        <td><%= daily_datum.total_ducks %></td>
        <td><%= daily_datum.hours_of_daylight %></td>
        <td><%= daily_datum.sunrise %></td>
        <td><%= daily_datum.moon_phase_id %></td>
        <td><%= daily_datum.moon_percent %></td>
        <td><%= daily_datum.weather %></td>
        <td><%= daily_datum.rained %></td>
        <td><%= link_to 'Show', daily_datum %></td>
        <td><%= link_to 'Edit', edit_daily_datum_path(daily_datum) %></td>
        <td><%= link_to 'Destroy', daily_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'Moon', new_moon_phase_path %>
<%= link_to 'New Daily datum', new_daily_datum_path %>


